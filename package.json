{
  "name": "@rei/cdr-tokens",
  "version": "11.0.0",
  "description": "Tokens for REI cedar design system",
  "author": "REI Software Engineering",
  "homepage": "https://rei.github.io/rei-cedar-tokens/#/",
  "repository": {
    "type": "git",
    "url": "https://github.com/rei/rei-cedar-tokens.git"
  },
  "license": "ISC",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/js/cdr-tokens.common.js",
  "module": "dist/js/cdr-tokens.mjs",
  "exports": {
    ".": {
      "import": "./dist/js/cdr-tokens.mjs",
      "require": "./dist/js/cdr-tokens.common.js"
    },
    "./dist/": "./dist/"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prerelease": "cross-env NODE_ENV=prerelease npm-publish-prerelease",
    "build:tokens": "npm run clean && node ./style-dictionary/build.js",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run validate && npm run build:tokens && npm run site-tokens",
    "build:grid": "node grid.js",
    "site-tokens": "node ./siteTokens.js",
    "validate": "node ./validate.js"
  },
  "devDependencies": {
    "concat": "^1.0.3",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "npm-publish-prerelease": "^1.0.2",
    "query-string": "^6.14.1",
    "require-json5": "^1.1.0",
    "rimraf": "^3.0.2",
    "style-dictionary": "^3.7.1",
    "tinycolor2": "^1.4.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
